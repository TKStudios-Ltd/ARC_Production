    {% if settings.password != blank and settings.collection_protected != blank %}
    <script>
      var col = document.querySelector('.collection--root.col_found'),
      	  pro = document.querySelector('.product-page--root.pro_found'),
          rec_pro = document.querySelector('.product-recommendations--root.pro_found'),
          stamp_widget = document.querySelector('.product-stampped-widget.pro_found'),
          form_overlay = document.querySelector('.form-overlay'),
      	  form = document.querySelector('.form-container');
            
          
      function product_grid(){
        var elem = document.querySelector('.color_images.active .product-page--grid');
        var msnry = new Masonry(elem, {
          itemSelector: '.product-media--wrapper',
          percentPosition: true,
          horizontalOrder: true,
          columnWidth: '.product-media--wrapper'
        });         
        document.querySelectorAll('.color_images_carousel .carousel--y-container').forEach( e => {
          e.style.height = 0;
        });
        var tallest = document.querySelector('.color_images_carousel.active .carousel--y-container .carousel--block').offsetHeight;
        document.querySelector('.color_images_carousel.active .carousel--y-container').style.height = tallest + "px";
        var root_new = document.querySelector('.color_images_carousel.active .carousel--root').offsetWidth;
        var dot_nav = document.querySelector('.color_images_carousel.active .carousel--dot-nav').offsetWidth;
        var trans = (root_new - dot_nav) / 2;
        document.querySelector('.color_images_carousel.active .carousel--dot-nav').style.transform = "translateX(" + trans+ "px)";
      }
      
      function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0)==' ') c = c.substring(1);
          if(c.indexOf(name) == 0)
            return c.substring(name.length,c.length);
        }
        return "";
      }

      function setCookie(name,value,days) {
        var expires = "";
        if (days) {
          var date = new Date();
          date.setTime(date.getTime() + (days*24*60*60*1000));
          expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (value || "")  + expires + "; path=/";

        document.body.setAttribute('cookie_set','true');

        if ( document.querySelector('.collection--root.col_found')){
          col.style.display = "block";
        }
        if ( document.querySelector('.product-page--root.pro_found')){
          pro.style.display = "flex";
          product_grid();
          rec_pro.style.display = "block";
          stamp_widget.style.display = "block";
        }
        form_overlay.style.display = "none";
        form.style.display = "none";
        document.body.classList.add("pswd-active");

        var css = '.featured-collection--root .product--root:nth-child(-n+4) {display: block;}.featured-collection--root .product--root {display: none;}.product--root {display: block;}',
            head = document.getElementsByTagName('head')[0],
            style = document.createElement('style');

        style.type = 'text/css';
        style.id = 'custom';

        style.textContent = css;
        head.appendChild(style);
      }

      function checkCookie() {
        var colpswd = getCookie("secure_password");
        var css = '.featured-collection--root .product--root:nth-child(-n+4) {display: block;} .featured-collection--root .product--root {display: none;}.product--root {display: block;}',
            head = document.getElementsByTagName('head')[0],
            style = document.createElement('style');

        style.type = 'text/css';
        style.id = 'custom';

        if (colpswd == 1) {
          if ( document.querySelector('.collection--root.col_found')){
            col.style.display = "block";
          }
          if ( document.querySelector('.product-page--root.pro_found')){
            pro.style.display = "flex";
            product_grid();
            rec_pro.style.display = "block";
            stamp_widget.style.display = "block";
          }
          if (form) {
            form_overlay.style.display = "none";
            form.style.display = "none";
          }
          document.body.classList.remove("pswd-active");
          
          var css = '.featured-collection--root .product--root:nth-child(-n+4) {display: block;}.featured-collection--root .product--root {display: none;}.product--root {display: block;}',
              head = document.getElementsByTagName('head')[0],
              style = document.createElement('style');

          style.type = 'text/css';
          style.id = 'custom';

          style.textContent = css;
          head.appendChild(style);
          
        } else {
          if ( document.querySelector('.collection--root.col_found')){
            col.style.display = "none";
          }
          if ( document.querySelector('.product-page--root.pro_found')){
            pro.style.display = "none";
            rec_pro.style.display = "none";
            stamp_widget.style.display = "none";
          }
          if (form) {
            form.style.display = "block";
            form.style.opacity = 1; 
          }
          
          var css = '[data-cookie_found="true"]{display:none;}',
              head = document.getElementsByTagName('head')[0],
              style = document.createElement('style');

          style.type = 'text/css';
          style.id = 'custom';
          style.textContent = css;
          head.appendChild(style);
          
        }
      }
      checkCookie();
    </script>
    {% endif %}
